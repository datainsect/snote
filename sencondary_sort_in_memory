val cook = sc.textFile("DataSet/cook.txt")
val cookSplited = cook.map(_.split(" ").map(_.toInt))
val cookGrouped = cookSplited.map(line =>(line(0),(line(1),line(2)))).groupByKey
val cookSorted = cookGrouped.sortByKey()

def sortPairList(list:Iterable[(Int,Int)]) = {
  list.toList.sortWith(_._1<_._1);
}
//t必须是完整的，不能分开写（t1,t2）,否则报类型错误？？为什么呢
val result=cookSorted.map{ t =>(t._1,sortPairList(t._2).map(_._2)) })




